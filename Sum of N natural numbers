public class naturalno {
    
        public static int sumOfNaturalNumber(int n){
        int sum = 0;
        for(int i = 1 ; i <= n ; i++){
            sum = sum + i;
        }
        return sum;
    }
    public static void main(String[] args) {
        int n = 10;
        int ans = sumOfNaturalNumber(n);
        System.out.println(ans);
        
    }
    
}
